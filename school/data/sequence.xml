<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for school.students -->
        <record id="seq_school_students" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">school.students</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for school.doctors -->
        <record id="seq_school_doctors" model="ir.sequence">
            <field name="name">Doctors Sequence</field>
            <field name="code">school.doctors</field>
            <field name="prefix">D</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for school.calendar -->
        <record id="seq_school_calendar" model="ir.sequence">
            <field name="name">Calendar</field>
            <field name="code">school.calendar</field>
            <field name="prefix">FDTS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
